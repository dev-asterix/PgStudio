{
  "all": true,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.test.ts",
    "src/test/**",
    "src/activation/**",
    "**/*.d.ts",
    "node_modules/**"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov",
    "json"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./.nyc_output",
  "check-coverage": false,
  "lines": 50,
  "statements": 50,
  "functions": 50,
  "branches": 50,
  "per-file": false,
  "cache": true,
  "produce-source-map": true,
  "instrument": true,
  "require": [
    "ts-node/register"
  ],
  "extension": [
    ".ts"
  ]
}
